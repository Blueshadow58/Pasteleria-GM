{
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "react-native",
        "javscript"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "quotes": [
            "error",
            "double"
        ]
    }
}